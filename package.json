{
  "name": "sms-backend",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && copyfiles src/ca-certificate.crt dist",
    "serve": "ts-node-dev --respawn --transpile-only src/app.ts",
    "start": "node dist/app.js",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate src/migrations/Migration -d src/data-source.ts",
    "migration:run": "RUN_MIGRATIONS=true ts-node src/app.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/express": "^4.17.21",
    "axios": "^1.9.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "detect-port": "^2.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "node-schedule": "^2.1.1",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.15.3",
    "@types/node-schedule": "^2.1.7",
    "copyfiles": "^2.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
